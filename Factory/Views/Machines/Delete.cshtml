@{
  Layout = "_Layout";
}

@model Factory.Models.Machine

<div class = "container">
  <div class = "infoBox">
    <h2>Are you sure you want to <strong>DELETE</strong> this Machine?</h2>

    <h2>Machine @Html.DisplayNameFor(model => model.Name): @Html.DisplayFor(model => model.Name)</h2>
    @using (Html.BeginForm())
    {
      <input type = "submit" value = "Delete" class = "btn2"/>
    }
    <h5>@Html.ActionLink("Cancel", "Index", null, null, new { @class = "btn" })</h5>
  </div>
</div>